{
  "multipart": [
    {
      "apply": { "model": "lavalamp:block/pipe_base" }
    },
    {   "when": { "north": true },
      "apply": { "model": "lavalamp:block/pipe_side" }
    },
    {   "when": { "east": true },
      "apply": { "model": "lavalamp:block/pipe_side", "y": 90, "uvlock": true  }
    },
    {   "when": { "west": true },
      "apply": { "model": "lavalamp:block/pipe_side", "y": 270, "uvlock": true  }
    },
    {   "when": { "south": true },
      "apply": { "model": "lavalamp:block/pipe_side", "y": 180, "uvlock": true  }
    },
    {   "when": { "up": true },
      "apply": { "model": "lavalamp:block/pipe_up" }
    },
    {   "when": { "down": true },
      "apply": { "model": "lavalamp:block/pipe_down" }
    }
  ]
}
